pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        script {
          echo 'Hello World'
        }
      }
    }
    
    stage('Test') {
      steps {
        script {
          echo 'Test'
        }
      }
    }

    stage('Deploy') {
      steps {
        script {
          echo 'Deploy'
        }
      }
    }
  }
}